            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.threads.JMeterContextService
import java.util.Base64

// Get the thread number
int threadNumber = ctx.getThreadNum()

threadNumber = threadNumber + 100

// Construct the username with &apos;u&apos; + threadNumber
String username = &quot;u${threadNumber}&quot;

// Define the password
String password = &quot;password&quot;

// Combine username and password
String credentials = &quot;${username}:${password}&quot;

// Encode credentials in Base64
String encodedCredentials = Base64.getEncoder().encodeToString(credentials.getBytes(&quot;utf-8&quot;))

// Set the encoded credentials in a variable
vars.put(&quot;encodedCredentials&quot;, encodedCredentials)
//vars.put(&quot;encodedCredentials&quot;, username)</stringProp>
